<view:PageBase x:Class="OneDo.View.SettingsPage"
               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
               xmlns:view="using:OneDo.View"
               xmlns:model="using:OneDo.Model"
               DataContext="{Binding SettingsPage, Source={StaticResource ViewModelLocator}}"
               mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <StackPanel Margin="24,12">
            <TextBlock Text="Settings" Style="{ThemeResource HeaderTextBlockStyle}" />
        </StackPanel>
    </Grid>
</view:PageBase>
